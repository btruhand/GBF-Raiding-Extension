{
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "impliedStrict": true,
            "templateStrings": true
        }
    },
    "env": {
        "es6": true,
        "webextensions": true,
        "browser": true
    },
    "rules": {
        "no-console": "off"
    }
}