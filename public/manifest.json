{
  "name": "GBF Raiding Extension",
  "version": "1.0",
  "description": "Extension for raiding",
  "manifest_version": 2,
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo512.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://*.twitter.com"
  ],
  "browser_action": {
    "default_title": "GBF Raiding"
  },
  "background": {
    "scripts": [
      "static/js/background.js"
    ],
    "persistent": false
  },
  "options_page": "settings.html"
}